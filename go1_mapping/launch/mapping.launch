<?xml version="1.0"?>
<launch> 
    <node pkg="go1_mapping" type="pointcloud2_stamped" name="pointcloud_timestamp" output="screen"/>
    <node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="pointcloud_to_laserscan">
        <remap from="cloud_in" to="/rslidar_points_stamped" />
        <remap from="scan" to="/scan" />

        <param name="target_frame" value="rslidar"/>
        <param name="transform_tolerance" value="0.1"/>
        <param name="min_height" value="0.0"/>
        <param name="max_height" value="0.3"/>
        <param name="angle_min" value="-1.5708"/>
        <param name="angle_max" value="1.5708"/>
        <param name="range_min" value="0.12"/>
        <param name="range_max" value="10.0"/>
        <param name="scan_time" value="10.0"/>
    </node>

   <node pkg="slam_toolbox" type="async_slam_toolbox_node" name="slam_toolbox" output="screen">
      <rosparam command="load" file="$(find go1_mapping)/config/mapper_params_online_async.yaml" />
      <param name="mode" value="mapping"/>
      <param name="map_file_name" value="$(find go1_mapping)/maps/big_lab_serial"/>     
    </node> 


        <!-- Run the map server 
    <node name="map_server" pkg="map_server" type="map_server" args="$(find go1_mapping)/maps/test_farm.yaml"/>



     Run AMCL  
        <node pkg="amcl" type="amcl" name="amcl" output="screen">
              <param name="odom_frame_id" value="go1_odom"/>
              <param name="tf_broadcast" value="true"/>
              <param name="base_frame_id" value="base_link"/>
              <remap from="map" to="/map"/>
              <remap from="scan" to="/scan"/>

      </node> -->


    <node pkg="tf" type="static_transform_publisher" name="world_to_map_broadcaster" args="0 0 0 0 0 0 world map 100" /> 
    <!-- RVIZ--> 
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find go1_mapping)/config/go1_map.rviz"/> 
</launch>




   